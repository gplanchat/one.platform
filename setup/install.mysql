  `path` varchar(255) COLLATE utf8_bin NOT NULL,
  `content` text COLLATE utf8_bin NOT NULL,
  `website_id` tinyint(3) unsigned NOT NULL,
  PRIMARY KEY (`entity_id`),
  UNIQUE KEY `UNQ_CMS_CONFIG__PATH` (`path`,`website_id`),
